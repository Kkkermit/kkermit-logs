{
	"name": "js-quick-console",
	"displayName": "Kkermit-logs",
	"description": "Quickly print out log messages",
	"version": "0.0.3",
	"publisher": "Kkermit",
	"author": {
		"name": "Kkermit"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Kkkermit/kkermit-logs"
	},
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#C80000",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.27.0"
	},
	"categories": [
		"Programming Languages",
		"Debuggers",
		"Other"
	],
	"keywords": [
		"Python",
		"Print",
		"Log"
	],
	"activationEvents": [
		"onLanguage:javascript",
		"onLanguage:typescript",
		"onCommand:extension.log"
	],
	"main": "./out/extension",
	"contributes": {
		"languages": [
			{
				"id": "javascript",
				"aliases": [
					"JavaScript",
					"javascript",
					"js"
				],
				"extensions": [
					".js"
				]
			},
			{
				"id": "typescript",
				"aliases": [
					"TypeScript",
					"typescript",
					"ts"
				],
				"extensions": [
					".ts"
				]
			}
		],
		"commands": [
			{
				"command": "extension.log",
				"title": "Console.Log JavaScript Selection"
			}
		],
		"keybindings": [
			{
				"command": "extension.log",
				"key": "ctrl+shift+l",
				"mac": "cmd+shift+l",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^8.10.25",
		"tslint": "^5.8.0",
		"typescript": "^2.6.1",
		"vscode": "^1.1.37"
	}
}
